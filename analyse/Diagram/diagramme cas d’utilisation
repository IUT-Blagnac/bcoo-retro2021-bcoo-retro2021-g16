@startuml

actor enseignants
usecase Profil_Enseignants as PE
usecase Modifier_Profil_Enseignants as MPE
usecase Identiters as IE
usecase Module as ME
usecase Specialiter 

actor Secretariat

actor Eleve 

enseignants -- MPE
PE ..> MPE : extend 
Secretariat -- MPE 
Eleve -- PE : consulter
PE -- IE
PE -- ME
PE -- Specialiter

@enduml
